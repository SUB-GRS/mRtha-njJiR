{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": "src/**, api-page/**"
      }
    }
  ],
  "routes": [
    {
      "src": "/src/(.*)",
      "dest": "/src/$1"
    },
    {
      "src": "/openapi.json",
      "dest": "/openapi.json"
    },
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ]
}
