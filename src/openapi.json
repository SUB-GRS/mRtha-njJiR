  "paths": {
    "/tools/hd": {
    "get": {
      "summary": "tools Upscale (Direct View)",
      "description": "Upscale an image and return the result as a direct image stream.",
      "tags": ["Tools"],
      "parameters": [
        {
          "name": "imageUrl",
          "in": "query",
          "required": true,
          "description": "URL of the image to upscale",
          "schema": {
            "type": "string"
          }
        }
      ],
      "responses": {
        "200": {
          "description": "The upscaled image file",
          "content": {
            "image/jpeg": {}
          }
        }
      }
    }
  },
    "/tools/ocr": {
    "get": {
      "summary": "OCR Text Extraction",
      "description": "Extract text from an image URL using AI Optical Character Recognition.",
      "tags": ["Tools"],
      "parameters": [
        {
          "name": "imageUrl",
          "in": "query",
          "required": true,
          "description": "Direct URL of the image to be processed (JPG/PNG)",
          "schema": {
            "type": "string",
            "example": "https://raw.githubusercontent.com/otiai10/gosseract/master/test/data/out.png"
          }
        }
      ],
      "responses": {
        "200": {
          "description": "Text extracted successfully",
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "success": { "type": "boolean" },
                  "data": {
                    "type": "object",
                    "properties": {
                      "extractedText": { "type": "string" }
                    }
                  },
                  "creator": { "type": "string" }
                }
              }
            }
          }
        },
        "400": { "description": "Missing imageUrl parameter" },
        "500": { "description": "Failed to fetch image or OCR processing error" }
      }
    }
  },
    "/ai/chatgpt": {
    "get": {
      "summary": "AI Chat (GPT-5 Nano)",
      "description": "Artificial Intelligence chat assistant using GPT-5 Nano model via chatgpt.",
      "tags": ["AI"],
      "parameters": [
        {
          "name": "prompt",
          "in": "query",
          "required": true,
          "description": "Your question or message to the AI",
          "schema": {
            "type": "string",
            "example": "Apa itu teori relativitas?"
          }
        }
      ],
      "responses": {
        "200": {
          "description": "Success",
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "status": { "type": "boolean" },
                  "data": {
                    "type": "object",
                    "properties": {
                      "model": { "type": "string" },
                      "reply": { "type": "string" }
                    }
                  }
                }
              }
            }
          }
        },
        "500": { "description": "failed or AI error" }
      }
    }
  },
  "/download/twitter": {
    "get": {
      "summary": "Twitter/X Downloader",
      "description": "Download videos, images, or audio from Twitter (X) status URL.",
      "tags": ["Downloader"],
      "parameters": [
        {
          "name": "url",
          "in": "query",
          "required": true,
          "description": "The URL of the Tweet (Twitter/X)",
          "schema": {
            "type": "string",
            "example": "https://x.com/SpaceX/status/1881403444059152433"
          }
        }
      ],
      "responses": {
        "200": {
          "description": "Media found",
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "status": { "type": "boolean" },
                  "creator": { "type": "string" },
                  "metadata": { "type": "object" },
                  "result": { "type": "array" },
                  "response_time": { "type": "string" }
                }
              }
            }
          }
        },
        "400": { "description": "Invalid URL format" },
        "500": { "description": "Server or Scraper error" }
      }
    }
 },
      "/ai/copilot": {
  "get": {
    "summary": "Copilot Chat",
    "description": "Kirim pertanyaan ke Copilot Microsoft dan dapatkan jawaban beserta referensi/citations",
    "tags": ["AI"],
    "parameters": [
      {
        "name": "message",
        "in": "query",
        "required": true,
        "description": "Pesan atau pertanyaan yang ingin dikirim ke Copilot",
        "schema": { "type": "string" }
      },
      {
        "name": "model",
        "in": "query",
        "required": false,
        "description": "Model Copilot yang digunakan (default: 'default', opsional: 'think-deeper', 'gpt-5')",
        "schema": { "type": "string", "enum": ["default", "think-deeper", "gpt-5"], "default": "default" }
      }
    ],
    "responses": {
      "200": {
        "description": "Berhasil mendapatkan jawaban dari Copilot",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "text": { "type": "string", "example": "Berikut adalah berita terbaru dari Indonesia..." },
                "citations": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "title": { "type": "string", "example": "Detik News" },
                      "icon": { "type": "string", "example": "https://example.com/icon.png" },
                      "url": { "type": "string", "format": "uri", "example": "https://www.detik.com/..." }
                    }
                  }
                }
              }
            }
          }
        }
      },
      "400": {
        "description": "Request invalid, message kosong",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "status": { "type": "boolean", "example": false },
                "error": { "type": "string", "example": "Message wajib diisi" }
              }
            }
          }
        }
      },
      "500": {
        "description": "Gagal memproses request",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "status": { "type": "boolean", "example": false },
                "error": { "type": "string", "example": "Gagal memproses request" }
              }
            }
          }
        }
      }
    }
  }
},
